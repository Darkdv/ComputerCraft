swapStacks: 
  returnTypes: 
  args: 
    1: 
      type: NUMBER
      description: The first slot
      name: from
    2: 
      type: NUMBER
      description: The other slot
      name: to
    3: 
      optional: true
      description: 
      type: STRING
      name: fromDirection
    4: 
      optional: true
      description: 
      type: STRING
      name: fromDirection
  description: Swap two slots in the inventory
  source: inventory
